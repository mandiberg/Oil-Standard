{
	"manifest_version": 2,
	"name": "Oil Standard",
	"version": "0.1",
	"permissions": [
		"storage"
	],
  "content_scripts": [{
    "matches": [
      "<all_urls>"
    ],
    "js": ["content.js"]
  }],
  "converted_from_user_script": true,
	"browser_action": {
   "default_icon": "oil.png",
   "default_popup": "popup.html"
  }
}
