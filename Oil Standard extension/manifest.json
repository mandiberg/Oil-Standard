{
  "manifest_version": 2,
  "name": "Oil Standard",
  "version": "0.3.1",
  "permissions": [
    "storage"
  ],
  "icons": {
    "128": "oil.png"
  },
  "content_scripts": [{
    "matches": [
      "<all_urls>"
    ],
    "exclude_matches": ["https://docs.google.com/*", "https://drive.google.com/*"],
    "js": ["content.js"]
  }],
  "converted_from_user_script": true,
  "browser_action": {
    "default_icon": "oil.png",
    "default_popup": "popup.html"
  }
}
